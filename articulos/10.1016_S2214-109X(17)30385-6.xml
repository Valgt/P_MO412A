            <ce:title id="cetitle10">Lessons for the Sustainable Development Goals from Ethiopia's success: the case of under-5 mortality</ce:title>
            <ce:author-group id="aug10">
               <ce:author id="au10" author-id="S2214109X17303856-64e88d5453d49d5f5ce8b7d742542bfe">
                  <ce:given-name>Yohannes Adama</ce:given-name>
                  <ce:surname>Melaku</ce:surname>
                  <ce:cross-ref id="cecref10" refid="aff1">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref id="cecref20" refid="aff2">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:author id="au20" author-id="S2214109X17303856-9c6a281d3c9f8a1f464a0969710d2492">
                  <ce:given-name>Zumin</ce:given-name>
                  <ce:surname>Shi</ce:surname>
                  <ce:cross-ref id="cecref30" refid="aff1">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:affiliation id="aff1" affiliation-id="S2214109X17303856-26392500dab3e899206f0270c6cd1350">
                  <ce:label>a</ce:label>
                  <ce:textfn>Adelaide Medical School, The University of Adelaide, Adelaide, SA 5005, Australia</ce:textfn>
                  <sa:affiliation>
                     <sa:organization>Adelaide Medical School</sa:organization>
                     <sa:organization>The University of Adelaide</sa:organization>
                     <sa:city>Adelaide</sa:city>
                     <sa:state>SA</sa:state>
                     <sa:postal-code>5005</sa:postal-code>
