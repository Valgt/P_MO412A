                  </sa:affiliation>
               </ce:affiliation>
               <ce:affiliation id="aff2" affiliation-id="S2214109X17303856-6ce32ef932678558136b9d3984fcc862">
                  <ce:label>b</ce:label>
                  <ce:textfn>Institute of Public Health, University of Gondar, Gondar, Ethiopia</ce:textfn>
                  <sa:affiliation>
                     <sa:organization>Institute of Public Health</sa:organization>
                     <sa:organization>University of Gondar</sa:organization>
                     <sa:city>Gondar</sa:city>
