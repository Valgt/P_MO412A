                     <sa:country>Australia</sa:country>
                  </sa:affiliation>
                  <ce:source-text id="sourcetext1a">General Medicine, Western Health, Victoria</ce:source-text>
               </ce:affiliation>
               <ce:correspondence id="cor1">
