            <ce:title id="cetitle10">An equitable challenge: when sustainable development goals set the post‐2015 agenda</ce:title>
            <ce:author-group id="aug10">
               <ce:author id="azph12131-cr-0001" author-id="S132602002300835X-6a2788698ac5f510990a157ad9de6ee5">
                  <ce:given-name>Eloise</ce:given-name>
                  <ce:surname>Williams</ce:surname>
                  <ce:cross-ref id="cecref10" refid="azph12131-aff-0001">
                     <ce:sup loc="post">1</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref id="cecref20" refid="cor1">*</ce:cross-ref>
                  
               </ce:author>
               <ce:affiliation id="azph12131-aff-0001" affiliation-id="S132602002300835X-373638c23647fb81dfffd97b4915436f">
