            <ce:title id="title0010">Editorial Overview: Toward new sustainable development goals using advanced materials on green technologies and the processes in Asia</ce:title>
            <ce:author-group id="augrp0010">
               <ce:author id="au1" biographyid="vt1" author-id="S2452223620301371-a3e59a7b8d360926d603479ca19c104e">
